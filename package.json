{
  "name": "classy-decorator",
  "version": "1.0.0",
  "description": "Create an instance of your javascript class without `new` keyword because this is more classy ;)",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "npm run compile && mocha --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digital-flowers/classy-decorator.git"
  },
  "keywords": [
    "class",
    "instance",
    "without",
    "new",
    "classy"
  ],
  "author": "Fareed Alnamrouti <digital.flowers@hotmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/digital-flowers/classy-decorator/issues"
  },
  "homepage": "https://github.com/digital-flowers/classy-decorator#readme",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "mocha": "^3.4.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0"
  }
}
